# Expression Atlas API Configuration
CACHE_DIR=data/cache
CACHE_EXPIRY=86400  # 24 hours in seconds

# API Rate Limiting
MAX_RETRIES=3
RETRY_DELAY=5  # seconds

# Data Processing
BATCH_SIZE=1000
MAX_RESULTS=10000

